{
  "manifest_version": 3,
  "name": "YTDislikes",
  "version": "1.1.0",

  "description": "Adds a dislike counter to youtube videos.",

  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "run_at": "document_start",
      "all_frames": true,
      "js": ["dislikes.js"]
    }
  ]

}
